{
    "manifest_version": 2,
    "name": "MemeTension",
    "description": "Spice up your casual conversations with dank memes. Just copy & paste into chat.",
    "version": "1.1.4",
    "icons": {
        "128": "logo_128.png"
    },
    "author": "Dominik Juriga",
    "browser_action": {
        "default_icon": "logo.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "cookies",
        "http://*.google.com/"
    ]
}